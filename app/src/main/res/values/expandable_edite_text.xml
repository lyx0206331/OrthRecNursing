<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableEditText">
        <attr name="eet_pre_textSize" format="dimension|reference" />
        <attr name="eet_pre_textColor" format="color|reference" />
        <attr name="eet_pre_textStyle" format="enum">
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="eet_suf_textSize" format="dimension|reference" />
        <attr name="eet_suf_textColor" format="color|reference" />
        <attr name="eet_suf_textStyle" format="enum">
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="eet_center_textSize" format="dimension|reference" />
        <attr name="eet_center_textColor" format="color|reference" />
        <attr name="eet_center_textStyle" format="enum">
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="eet_isCenterEditable" format="boolean" />
        <attr name="eet_pre_to_center_spacing" format="dimension|reference" />
        <attr name="eet_suf_to_center_spacing" format="dimension|reference" />
        <attr name="eet_pre_text" format="string|reference" />
        <attr name="eet_suf_text" format="string|reference" />
        <attr name="eet_center_text" format="string|reference" />
        <attr name="eet_center_inputType" format="enum">
            <enum name="text" value="1" />
            <enum name="number" value="2" />
            <enum name="phone" value="3" />
            <enum name="dateTime" value="4" />
        </attr>
        <attr name="eet_center_hint" format="string|reference" />
        <attr name="eet_center_hint_color" format="color|reference" />
    </declare-styleable>
</resources>